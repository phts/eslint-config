{
  "name": "@phts/eslint-config",
  "version": "1.8.0",
  "description": "My config for ESLint.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:all": "eslint --no-ignore --ext .js,.jsx,.ts,.tsx .",
    "preversion": "npm run lint && npm test",
    "test": "jest"
  },
  "peerDependencies": {
    "eslint": "^5, ^6"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/phts/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Phil Tsaryk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phts/eslint-config/issues"
  },
  "homepage": "https://github.com/phts/eslint-config#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3"
  },
  "devDependencies": {
    "@phts/prettier-config": "^1.0.4",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "eslint": "^6.2.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/playground/"
    ]
  }
}
